Create table -lauseet

CREATE TABLE Resurssi (
	id int NOT NULL AUTO_INCREMENT,
	PRIMARY KEY (auto_id),
	kayttoaikaalkaa TIME,				# kellonaika jolloin on mahdollista tehdä ensimmäinen varaus
	kayttoaikapaattyy TIME,
	varausyksikko int,
	resurssin_nimi varchar(8),
	tuntihinta int
);

CREATE TABLE Varaus (
	id int NOT NULL AUTO_INCREMENT,
	tunnusluku int,
	alkamisajankohta datetime,
	PRIMARY KEY (varaus_id),
	resurssi_id int,
	FOREIGN KEY (resurssi_id) REFERENCES Resurssi(id) ON DELETE CASCADE,
	asukas_id int,
	FOREIGN KEY (asukas_id) REFERENCES Kayttaja(id) ON DELETE CASCADE,
	lasku_id int,
	FOREIGN KEY (lasku_id) REFERENCES Lasku(id) 
	
);

CREATE TABLE Kayttaja ( 
	id int NOT NULL AUTO_INCREMENT,
	nimi varchar(64),
	salasana varchar(64),
	asunto varchar(64),
);

CREATE TABLE Lasku (
	id int NOT NULL AUTO_INCREMENT,
	PRIMARY KEY (id),
	maksettu boolean,
	viitenumero 
);